msgstr "%(playmode)s、スタート!"

#: ssAppMain.py:271
msgid "Game result"
msgstr "今回の結果"

#: ssAppMain.py:272
#, python-format
msgid "Final score: %(score)d"
msgstr "最終得点: %(score)d"

#: ssAppMain.py:273
#, python-format
