msgstr "パンチ: %(punches)d, ヒット: %(hits)d, ヒット率: %(accuracy).2f%%"

#: ssAppMain.py:319
#, python-format
msgid "Game started at %(startedtime)s!"
msgstr "%(startedtime)sにゲームを開始!"

#: ssAppMain.py:340
#, python-format
msgid "Game over! Final score: %(score)d"
msgstr "ゲームオーバー! 最終得点: %(score)d"

